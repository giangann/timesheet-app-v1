{
  "expo": {
    "name": "CPV Timesheet",
    "slug": "timesheet-app-v1",
    "version": "1.0.0",
    "icon": "./assets/images/app-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo-vptw-300.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.giangann229.timesheetappv1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-adaptive-icon.png",
        "backgroundColor": "#FF0000"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.giangann229.timesheetappv1",
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "icon": "./assets/images/app-icon.png",
      "color": "#FF0000"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "timesheet-app-v1",
          "organization": "free-tg"
        }
      ],
      "expo-font",
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "DEFAULT"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7eecf26f-0091-409d-8b74-e94bd67f07d0"
      }
    },
    "runtimeVersion":"1.0.0",
    "updates": {
      "url": "https://u.expo.dev/7eecf26f-0091-409d-8b74-e94bd67f07d0"
    },
    "newArchEnabled": true
  }
}
